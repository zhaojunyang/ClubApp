        <activity
            android:name="com.geminno.club.activity.MybbsActivity"
            android:label="@string/title_activity_mybbs" >
        </activity>
        <activity
            android:name="com.geminno.club.activity.MystoreActivity"
            android:label="@string/title_activity_mystore" >
        </activity>
        <activity
            android:name="com.geminno.club.activity.MyclubsActivity"
            android:label="@string/title_activity_myclubs" >
