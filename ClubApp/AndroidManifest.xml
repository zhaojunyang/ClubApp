        <activity
            android:name="com.geminno.club.activity.MyactivitiesActivity"
            android:label="@string/title_activity_myactivities" >
        </activity>
        <activity
            android:name="com.geminno.club.activity.MyfriendsActivity"
            android:label="@string/title_activity_myfriends" >
