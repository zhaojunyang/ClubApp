        <activity
            android:name="com.geminno.club.activity.ActivityEnterSuccessActivity"
            android:label="@string/title_activity_activity_enter_success" >
        </activity>
        
        <!-- 石然 -->
        <activity
            android:name="com.geminno.club.activity.LoginActivity"
            android:label="@string/title_activity_login" >
            
