        <activity
            android:name="com.geminno.club.activity.InvitationDetailActivity"
            android:label="@string/title_activity_invitation_detail" >
        </activity>
        <!-- 俱乐部主页 -->
        <activity
            android:name="com.geminno.club.activity.ClubMainPageActivity"
            android:label="@string/title_activity_club_main_page"
            android:theme="@android:style/Theme.Holo.NoActionBar" >
        </activity>
